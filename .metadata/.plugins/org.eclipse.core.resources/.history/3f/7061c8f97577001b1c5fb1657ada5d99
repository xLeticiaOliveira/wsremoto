package br.com.concessionaria.modelo;

public class Carro {
	
	private float valor;
	private String cor;
	private boolean status;
	private short velocidadeAtual;
	private String modelo;
	
	
	public void preencherCor(String param) {
		cor = param;		
	}
	
	public String retornarCor() {
		return cor;
	}
 }
